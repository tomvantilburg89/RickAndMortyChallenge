{% extends 'base.html.twig' %}
{% block content %}
    {% for content in body %}

        <div class="pb-3">
            <h{{ loop.index }} class="text-3xl font-bold underline">
                {{ content.h }}
            </h{{ loop.index }}>
            {% for p in content.p %}
                <p class="mt-3">{{ p }}</p>
            {% endfor %}
        </div>
    {% endfor %}
{% endblock %}
