{% extends 'base.html.twig' %}
{% block content %}
    <ul role="list" class="divide-y divide-gray-100">
        {% for character in characters %}
            {% include 'parts/character.html.twig' with {'character': character} %}
        {% endfor %}
    </ul>
    {% if pagination %}
        {% include 'parts/pagination.html.twig' with {'pagination': pagination, 'paginationPath': 'characters'} %}
    {% endif %}
{% endblock %}